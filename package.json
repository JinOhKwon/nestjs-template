{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "'kjo's nestjs template",
  "author": "kjo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pra.studio": "npx prisma studio",
    "pra:gen": "prisma generate",
    "pra.mg:dev": "prisma migrate dev --name autoMigrate --preview-feature",
    "pra.mg:dev:create": "prisma migrate dev --create-only --preview-feature",
    "pra.mg:reset": "prisma migrate reset --preview-feature",
    "test": "jest --config ./jest.config.json --no-compilation-cache --silent --verbose --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --no-cache"
  },
  "prisma": {
    "schema": "./src/@submodule/prisma/schema.prisma"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/websockets": "^9.2.1",
    "@prisma/client": "^4.10.1",
    "@socket.io/redis-adapter": "^8.0.0",
    "aws-sdk": "^2.1253.0",
    "cache-manager": "^5.1.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nest-winston": "^1.8.0",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "socket.io": "^4.5.4",
    "sql-highlight": "^4.3.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.5",
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.188",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "bcrypt": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "prisma": "^4.10.1",
    "prisma-class-generator": "^0.2.5",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.5.0",
    "prisma-erd-generator": "^1.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
